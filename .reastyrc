{
    "basedir": "src/modules",
    "newline": "\n",
    "indent": "  ",
    "file_write_options": "utf8",
    "files": {
        ".": {
            "filename": "{{NAME}}",
            "styleImport": "_",
            "content": "{{NEWLINE}}.{{NAME}}{{NEWLINE}}{{INDENT}}// your code here",
            "extension": "styl"
        },
        "_": {
            "filename": "__",
            "content": "// your vars and mixins here{{NEWLINE}}",
            "extension": "styl"
        },
        "z": {
            "filename": "{{NAME}}",
            "extension": "js",
            "content": "import React from 'react';{{NEWLINE}}{{NEWLINE}}class {{NAME}} extends React.Component {{{NEWLINE}}{{INDENT}}{{INDENT}}render() {{{NEWLINE}}{{INDENT}}{{INDENT}}{{INDENT}}{{INDENT}}return (){{NEWLINE}}{{INDENT}}{{INDENT}}}{{NEWLINE}}}{{NEWLINE}}{{NEWLINE}}export default {{NAME}};"
        }
    }
}
